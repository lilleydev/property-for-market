<h4> Rooms New Page </h4>

<%= render partial: 'layouts/errors', locals: {object: @room}  %>




<%= form_for @room do |f| %>

<div class="field">
  <%= f.label :name %>
  <%= f.text_field :name %>
</div>

<div class="field">
  <%= f.label :description %>
  <%= f.text_field :description %>
</div>

<div class="field">
  <%= f.label :features %>
  <%= f.text_field :features %>
</div>

<%= f.submit %>

<% end %>