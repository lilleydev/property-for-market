<% if !@room %>
<table>
  <tr>
    <th> Name </th>
    <th> Description </th>
    <th> Person's Contact </th>

  </tr>
  <% current_user.helped_tasks.each do |t| %>
  <tr>
    <td> <%= t.name %> </td>
    <td> <%= t.description %></td>
    <td> <%= t.user.email %></td>



  </tr>
  <% end %>
</table>
<%end%>